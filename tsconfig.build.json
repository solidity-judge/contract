{
    "compilerOptions": {
        "experimentalDecorators": true,
        "target": "esnext",
        "module": "esnext",
        "removeComments": true,
        "sourceMap": true,
        "incremental": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true
    },
    "include": ["SDK/**/*", "typechain-types/**/*"],
}
